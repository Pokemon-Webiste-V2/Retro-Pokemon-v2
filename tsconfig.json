{
  "compilerOptions": {
    "jsx": "react",
    "ignoreDeprecations": "5.0",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true
  },
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "exclude": ["**/*.spec.ts", "**/*.spec.tsx", "node_modules"]
}